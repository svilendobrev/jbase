#builtin misses some syntaxes. and avoid any of it
CTAGSflags = --langmap=matlab: --langmap=ObjectiveC: --langmap=C++:
#and force the objc1 ONLY
tag tags: iostags
iostags: plaintags
	ctags -a --options=objc1.ctags -R $(SRCobjc) --totals

